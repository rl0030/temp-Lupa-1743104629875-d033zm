{
  "name": "lupa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env NODE_ENV=development ENV_FILE=.env.development react-native run-android",
    "ios": "cross-env NODE_ENV=development ENV_FILE=.env.development react-native run-ios",
    "ios-16promax": "APP_ENV=development NODE_ENV=development ENV_FILE=.env.development react-native run-ios --simulator='iPhone 15 Plus' --port 8081",
    "ios-15promax": "APP_ENV=development NODE_ENV=development ENV_FILE=.env.development react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios-14promax": "cross-env NODE_ENV=development ENV_FILE=.env.development react-native run-ios --simulator='iPhone 14 Pro Max'",
    "ios-18-15promax": "APP_ENV=development NODE_ENV=development ENV_FILE=.env.development react-native run-ios --simulator='iPhone 15 Pro Max'",
    "android-dev": "APP_ENV=development cross-env NODE_ENV=development ENV_FILE=.env.development react-native run-android",
    "android-prod": "APP_ENV=production cross-env NODE_ENV=production ENV_FILE=.env.production react-native run-android --variant=release",
    "android-clean": "cd android && ./gradlew clean",
    "android-build": "cd android && ./gradlew assembleRelease",
    "android-build-dev": "cd android && ./gradlew assembleDebug",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android-clean-gradlew": "cd android && ./gradlew clean && cd .. && rm -rf android/app/build",
    "ios-prod": "APP_ENV=production  cross-env NODE_ENV=production ENV_FILE=.env.production react-native run-ios",
    "ios-15promax-prod": "APP_ENV=production cross-env NODE_ENV=production ENV_FILE=.env react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios-14promax-prod": "APP_ENV=production cross-env NODE_ENV=production ENV_FILE=.env.production react-native run-ios --simulator='iPhone 14 Pro Max'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "react-native-clean-project",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "postinstall": "patch-package",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace Lupa.xcworkspace -scheme Lupa -configuration Release -archivePath Lupa.xcarchive archive",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write .",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "generate-icons": "npx @svgr/cli --icon --native -d src/components/icons src/assets/icons",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "clear-cache": "react-native start --reset-cache",
    "nuclear-reset": "rm -rf node_modules/ && npm cache clean --force && npm install && cd ios && rm -rf Pods && pod install && cd .. && npm run ios-15promax"
  },
  "workspaces": [
    "functions"
  ],
